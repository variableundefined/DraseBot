generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        String      @id @db.VarChar
  updatedAt DateTime    @updatedAt @db.Timestamp(6)
  up        Decimal     @db.Decimal
  np        Int
  character character[]
}

model botban {
  id String @id @db.VarChar
}

model character {
  /// The value of this field is generated by the database as: `nextval('"Character_id_seq"'::regclass)`.
  id         Int     @id @default(autoincrement())
  userID     String? @db.VarChar
  name       String  @db.VarChar
  EFund      BigInt  @default(0)
  AFund      BigInt  @default(0)
  UsedUP     BigInt  @default(0)
  TotalUP    BigInt  @default(0)
  Occupation String  @default("\"None\"") @db.VarChar
  GSheet     String  @default("\"None\"") @db.VarChar
  Income     BigInt  @default(0)
  Thumbnail  String? @db.VarChar
  Users      Users?  @relation(fields: [userID], references: [id])
}

model link {
  id       Int     @default(autoincrement())
  name     String  @id @default("NULL") @db.VarChar
  link     String? @db.VarChar
  category String? @db.VarChar
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model characterlog {
  id            Int     @default(autoincrement())
  type          String  @db.VarChar
  sendcharid    Int?
  receivecharid Int
  desc          String? @db.VarChar
  reason        String? @default("\"No reason provided\"") @db.VarChar

  @@ignore
}
