generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        String      @id
  updatedAt DateTime    @updatedAt
  up        Float
  np        Int
  character character[]
}

model botban {
  id String @id
}

model character {
  /// The value of this field is generated by the database as: `nextval('"Character_id_seq"'::regclass)`.
  id         Int     @id @default(dbgenerated())
  userID     String?
  name       String
  EFund      Int     @default(0)
  AFund      Int     @default(0)
  UsedUP     Int     @default(0)
  TotalUP    Int     @default(0)
  Occupation String  @default("\"None\"")
  GSheet     String  @default("\"None\"")
  Income     Int     @default(0)
  Thumbnail  String?
  Users      Users?  @relation(fields: [userID], references: [id])
}

