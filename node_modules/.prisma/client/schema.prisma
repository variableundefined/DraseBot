generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int         @id
  updatedAt DateTime    @updatedAt
  up        Float
  np        Int
  Character Character[]
}

model Character {
  id         Int    @id @default(autoincrement())
  userID     Int?
  name       String
  EFund      Int    @default(0)
  AFund      Int    @default(0)
  UsedUP     Int    @default(0)
  TotalUP    Int    @default(0)
  Occupation String @default("\"None\"")
  GSheet     String @default("\"None\"")
  Income     Int    @default(0)
  Users      Users? @relation(fields: [userID], references: [id])
}
